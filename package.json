{
  "name": "megbailey.github.io",
  "homepage": "https://megbailey.github.io/",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^13.5.0",
    "antd": "^5.4.6",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.6.1",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.4",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build --branch main",
    "storybook": "npm install && storybook dev -p 6006 -s public",
    "build-storybook": "npm install && storybook build -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^7.0.3",
    "@storybook/addon-docs": "^7.0.3",
    "@storybook/addon-essentials": "^7.0.3",
    "@storybook/addon-interactions": "^7.0.3",
    "@storybook/addon-links": "^7.0.3",
    "@storybook/addon-mdx-gfm": "^7.0.3",
    "@storybook/node-logger": "^7.0.3",
    "@storybook/preset-create-react-app": "^7.0.3",
    "@storybook/react": "^7.0.3",
    "@storybook/react-webpack5": "^7.0.3",
    "@storybook/testing-library": "^0.1.0",
    "babel-plugin-named-exports-order": "^0.0.2",
    "check-dependencies": "^1.1.0",
    "gh-pages": "^5.0.0",
    "storybook": "^7.0.3",
    "webpack": "^5.81.0"
  }
}
